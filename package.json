{
  "name": "basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "run-s build start",
    "debug": "tsc && npm run start",
    "build": "cross-env nammatham_env=build ts-node src/startup.ts && swc src -d dist",
    "tsc": "tsc",
    "build2": "swc src -d dist",
    "serve:watch": "nodemon --watch src --ext ts --exec 'npm run serve'",
    "build:watch": "nodemon --watch src --ext ts --delay 0.5 --exec 'npm run build'",
    "start:watch": "nodemon --watch dist --delay 0.3 --exec  'npm start'",
    "start": "func start",
    "dev": "run-s serve:watch"
  },
  "author": "Thada Wangthammang",
  "license": "MIT",
  "dependencies": {
    "@azure/functions": "latest",
    "inversify": "^6.0.1",
    "nammatham": "^0.5.2-alpha",
    "nodemon": "^2.0.20",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.35",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
